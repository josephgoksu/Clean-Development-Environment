{
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "boundary",
  "editor.cursorBlinking": "smooth",
  "files.autoSave": "off",
  "git.confirmSync": false,
  "window.zoomLevel": 1,
  "editor.renderLineHighlight": "all",
  "editor.fontFamily": "Fira Code, Menlo, Monaco, ‘Courier New’, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "terminal.integrated.fontFamily": "Operator Mono",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "eslint.autoFixOnSave": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/ios/Pods": true,
    "**/android/app/build": true,
    "**/android/app/src/main/assets": true,
    "**/android/.gradle": true,
    "**/android/.idea": true
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.idea": true
  },
  "git.enableSmartCommit": true,
  "emmet.showAbbreviationSuggestions": false,
  "extensions.ignoreRecommendations": false,
  "editor.detectIndentation": false,
  "files.associations": {
    "*.js": "javascript"
  },
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "git.autofetch": true,
  "gitlens.codeLens.scopes": ["document", "containers"],
  "gitlens.recentChanges.highlight.locations": ["gutter", "line", "overview"],
  "gitlens.keymap": "alternate",
  "gitlens.gitExplorer.view": "repository",
  "gitlens.hovers.currentLine.over": "line",
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "javascript.format.enable": true,
  "prettier.eslintIntegration": true,
  "javascript.validate.enable": false,
  "gitlens.historyExplorer.enabled": true,
  "workbench.iconTheme": "file-icons",
  "workbench.colorCustomizations": {},
  "todohighlight.keywords": ["REFACTOR", "FIXME", "BUG"],
  "gitlens.mode.active": "review",
  "terminal.integrated.rendererType": "dom",
  "workbench.statusBar.feedback.visible": false,
  "workbench.fontAliasing": "auto",
  "git.enableCommitSigning": true,
  "workbench.colorTheme": "One Dark+ (Sublime)",
  "workbench.editor.showTabs": true,
  "workbench.startupEditor": "newUntitledFile",
  "javascript.referencesCodeLens.enabled": true,
  "gitignore.cacheExpirationInterval": 3600
}
